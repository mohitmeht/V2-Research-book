@Article{QuickHartnig2009,
  author    = {Christian Quick and Dietrich Ritzinger and Werner Lehnert and Christoph Hartnig},
  title     = {Characterization of water transport in gas diffusion media},
  journal   = {Journal of Power Sources},
  year      = {2009},
  volume    = {190},
  number    = {1},
  pages     = {110--120},
  month     = {may},
  issn      = {03787753},
  abstract  = {An in-depth insight in the role of gas diffusion layers (GDLs) and its impact on the water management is a key issue for the optimization of fuel cells. A new ex situ test method is developed to investigate the water transport in gas diffusion media for polymer electrolyte membrane fuel cells (PEMFCs). This research is focused on properties of GDLs, which influence the water removal and water retention in the cell. Gas diffusion media are evaluated ex situ in terms of liquid water and water vapor transport employing a conventional PEMFC setup. The amount of water transported through a GDL and out of the cell is determined by the properties of the gas diffusion medium. GDL properties such as the GDL thickness have a significant impact on water transport behavior. Furthermore, high impregnation weight or an additional micro-porous layer (MPL) reduces water removal due to enhanced mass transport resistances. The composition and distribution of the impregnation material in the GDL substrate also play a crucial role. Water transport rates depend not only on the GDL properties but increase exponentially with cell temperature. Finally, a two-phase water transport model is proposed taking into account both diffusive gas phase and liquid water transport in diffusion media. Based on this model, ex situ data set in correlation with in situ performance in PEMFCs on dry operating conditions and guidelines towards new design concepts for gas diffusion media are deduced.},
  doi       = {10.1016/j.jpowsour.2008.07.093},
  keywords  = {CB,CCM,CL,FEP,GDL,Gas diffusion layer,MEA,MPL,PEM fuel cell,PEMFC,PTFE,Water management,Water transport,atm,atmosphere (ambient pressure),carbon black,catalyst layer,catalyst-coated membrane,fluorinated ethylene propylene copolymer,gas diffusion layer,in-plane direction,ip,membrane electrode assembly,micro-porous layer,polymer electrolyte membrane fuel cell,polytetrafluoroethylene,through-plane direction,tp,weight,wt},
  owner     = {amnesia},
  publisher = {Elsevier {BV}},
  timestamp = {2015.03.25},
  url       = {http://www.sciencedirect.com/science/article/pii/S0378775308015681},
}

@Article{RadinSiegel2015,
  author    = {Maxwell D. Radin and Charles W. Monroe and Donald J. Siegel},
  title     = {How Dopants Can Enhance Charge Transport in Li2O2},
  journal   = {Chemistry of Materials},
  year      = {2015},
  volume    = {27},
  number    = {3},
  pages     = {839--847},
  month     = dec,
  issn      = {0897-4756},
  abstract  = {The performance of high-capacity Li/O2 batteries is limited by the high overpotential associated with the oxygen evolution reaction (OER) during charging. These losses have been attributed to sluggish charge transport within the solid lithium peroxide (Li2O2) discharge phase. Recent experiments have shown that use of Co3O4-containing Li/O2 electrodes enhances rechargeability, but the mechanism responsible for this effect is unclear, as are the general prospects for the promotion of the Li/O2 OER. Here first-principles calculations are combined with continuum-scale transport theory to build a multiscale model which demonstrates that the incorporation of trace Co into Li2O2 is a plausible mechanism for OER promotion. These calculations suggest that doping at equilibrium levels (tens of ppm) can enhance charge transport by shifting the balance of Li-ion vacancies and hole polarons. This mechanism could rationalize the improved rechargeability observed in Li/O2 electrodes containing Co. On the basis of a computational assessment of 22 additional dopants, we speculate that Ni may also be an effective OER promoter. The performance of high-capacity Li/O2 batteries is limited by the high overpotential associated with the oxygen evolution reaction (OER) during charging. These losses have been attributed to sluggish charge transport within the solid lithium peroxide (Li2O2) discharge phase. Recent experiments have shown that use of Co3O4-containing Li/O2 electrodes enhances rechargeability, but the mechanism responsible for this effect is unclear, as are the general prospects for the promotion of the Li/O2 OER. Here first-principles calculations are combined with continuum-scale transport theory to build a multiscale model which demonstrates that the incorporation of trace Co into Li2O2 is a plausible mechanism for OER promotion. These calculations suggest that doping at equilibrium levels (tens of ppm) can enhance charge transport by shifting the balance of Li-ion vacancies and hole polarons. This mechanism could rationalize the improved rechargeability observed in Li/O2 electrodes containing Co. On the basis of a computational assessment of 22 additional dopants, we speculate that Ni may also be an effective OER promoter.},
  doi       = {10.1021/cm503874c},
  owner     = {amnesia},
  publisher = {American Chemical Society ({ACS})},
  timestamp = {2015.03.25},
}

