@Article{QiaoZhang2013,
  author    = {Qiao, Jinli and Xu, Li and Shi, Penghui and Zhang, Lei and Baker, Ryan and Zhang, Jiujun},
  title     = {{Effect of KOH concentration on the oxygen reduction kinetics catalyzed by heat-treated co-pyridine/C electrocatalysts}},
  journal   = {International Journal of Electrochemical Science},
  year      = {2013},
  volume    = {8},
  pages     = {1189--1208},
  issn      = {14523981},
  abstract  = {Using a simple thermal method, a non-noble metal electrocatalyst (Co-N-S/C) is successfully synthesized and its{\textquoteright} composition is analyzed using EDX. The electrochemical activity of such a catalyst is also tested using surface electrochemical methods. Three one-electron transfer redox processes in the studied potential range are identified, where only one such a process is associated with the catalyst's ORR activity. The effect of KOH on the Co-N-S/C catalyzed ORR kinetics is investigated using the rotating disk electrode technique in a concentration range of 0.05 to 6.0M. Several ORR kinetic parameters such as overall electron transfer number (n), the product of electron transfer number and coefficient in ORR rate determining step ( ? n ), and the exchange current density ? ( o i O H O ) are estimated. The results show that values of n at different KOH concentrations are close to 2 / 2 4.0, suggesting that Co-N-S/C catalyzed ORR is a 4-electron transfer process from O2 to H2O. KOH concentration has an insignificant effect on the value of n, however, a significant effect is observed on both values of ? n ? ? ? and o i O H O . ( n 2 / ? ? decreases while that of o i O H O increases with increasing KOH 2 / 2 2 concentration).},
  isbn      = {1452-3981},
  keywords  = {Alkaline polymer electrolyte membrane fuel cell.,Co-Pyridine/C catalyst,KOH concentration effect,Oxygen reduction reaction},
  owner     = {amnesia},
  timestamp = {2015.03.25},
}

