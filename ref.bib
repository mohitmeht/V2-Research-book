  keywords  = {Carbide derived carbon,Oxygen electroreduction,Polymer electrolyte membrane fuel cell,Tungsten carbide},
  owner     = {amnesia},
  publisher = {Elsevier {BV}},
  timestamp = {2015.03.25},
}

