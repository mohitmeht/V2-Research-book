@Article{WittmaierFriedrich2013,
  author    = {Dennis Wittmaier and Timo Danner and Norbert Wagner and K. Andreas Friedrich},
  title     = {Screening and further investigations on promising bi-functional catalysts for metal{\textendash}air batteries with an aqueous alkaline electrolyte},
