@Article{HuangKuang2005,
  author    = {Hui Huang and Wenkui Zhang and Meichao Li and Yongping Gan and Jinhua Chen and Yafei Kuang},
  title     = {Carbon nanotubes as a secondary support of a catalyst layer in a gas diffusion electrode for metal air batteries},
