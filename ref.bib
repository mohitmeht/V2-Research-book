@Article{LiMazza2008,
  author    = {Hui Li and Yanghua Tang and Zhenwei Wang and Zheng Shi and Shaohong Wu and Datong Song and Jianlu Zhang and Khalid Fatih and Jiujun Zhang and Haijiang Wang and Zhongsheng Liu and Rami Abouatallah and Antonio Mazza},
