@Article{GurauMann2007,
  author    = {Vladimir Gurau and Michael J. Bluemle and Emory S. De Castro and Yu-Min Tsou and Thomas A. Zawodzinski and J. Adin Mann},
  title     = {Characterization of transport properties in gas diffusion layers for proton exchange membrane fuel cells},
  journal   = {Journal of Power Sources},
