@Article{GullionPennington1998,
  author        = {Terry Gullion and Charles H Pennington},
  title         = {{\texttheta}-{REDOR}: an {MAS} {NMR} method to simplify multiple coupled heteronuclear spin systems},
  journal       = {Chemical Physics Letters},
  year          = {1998},
  volume        = {290},
  number        = {1-3},
  pages         = {88--93},
  month         = {jun},
  issn          = {00092614},
  __markedentry = {[mrmehta:]},
  abstract      = {The measurement of heteronuclear dipolar couplings with rotational-echo, double-resonance NMR (REDOR) is straightforward for isolated S–I spin pairs. However, measuring the n dipolar couplings for an S spin coupled simultaneously to n I spins is not easy because of additional complications arising from the orientational dependencies between the n dipolar tensors. We present an important modification of REDOR, called $\theta$-REDOR, that is useful for systems with multiple dipolar couplings. This new experiment recovers the n dipolar couplings while eliminating the complicating orientational dependence between the dipolar tensors of the REDOR experiment. A REDOR transform of data obtained by $\theta$-REDOR provides the n dipolar frequencies.},
  doi           = {10.1016/s0009-2614(98)00522-3},
  isbn          = {0009-2614},
  publisher     = {Elsevier {BV}},
  url           = {http://www.sciencedirect.com/science/article/pii/S0009261498005223},
}

