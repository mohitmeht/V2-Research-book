@Article{WangCui2012,
  author    = {Shan Wang and Shanmu Dong and Jun Wang and Lixue Zhang and Pengxian Han and Chuanjian Zhang and Xiaogang Wang and Kejun Zhang and Zhenggang Lan and Guanglei Cui},
