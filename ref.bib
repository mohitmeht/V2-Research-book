@Article{ShuiDai2014,
  author    = {Jianglan Shui and Feng Du and Chenming Xue and Quan Li and Liming Dai},
  title     = {Vertically Aligned N-Doped Coral-like Carbon Fiber Arrays as Efficient Air Electrodes for High-Performance Nonaqueous Li{\textendash}O2 Batteries},
