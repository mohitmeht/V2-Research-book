@Article{Evans2013,
  author        = {Cain Evans and Konstantinos Pappas and Fatos Xhafa},
  title         = {Utilizing artificial neural networks and genetic algorithms to build an algo-trading model for intra-day foreign exchange speculation},
  journal       = {Mathematical and Computer Modelling},
  year          = {2013},
