@Article{AntoliniPassalacqua1999,
  author    = {E. Antolini and L. Giorgi and A. Pozio and E. Passalacqua},
  title     = {Influence of Nafion loading in the catalyst layer of gas-diffusion electrodes for {PEFC}},
