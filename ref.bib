@Article{ZhaoZhang2012,
  author    = {Y. Zhao and L. Xu and L. Mai and C. Han and Q. An and X. Xu and X. Liu and Q. Zhang},
  title     = {Hierarchical mesoporous perovskite La0.5Sr0.5CoO2.91 nanowires with ultrahigh capacity for Li-air batteries},
  journal   = {Proceedings of the National Academy of Sciences},
  year      = {2012},
  volume    = {109},
  number    = {48},
  pages     = {19569--19574},
  month     = {nov},
  issn      = {1091-6490},
  abstract  = {Lithium-air batteries have captured worldwide attention due to their highest energy density among the chemical batteries. To provide continuous oxygen channels, here, we synthesized hierarchical mesoporous perovskite La(0.5)Sr(0.5)CoO(2.91) (LSCO) nanowires. We tested the intrinsic oxygen reduction reaction (ORR) and oxygen evolution reaction (OER) activity in both aqueous electrolytes and nonaqueous electrolytes via rotating disk electrode (RDE) measurements and demonstrated that the hierarchical mesoporous LSCO nanowires are high-performance catalysts for the ORR with low peak-up potential and high limiting diffusion current. Furthermore, we fabricated Li-air batteries on the basis of hierarchical mesoporous LSCO nanowires and nonaqueous electrolytes, which exhibited ultrahigh capacity, ca. over 11,000 mAh$\cdot$g(-1), one order of magnitude higher than that of LSCO nanoparticles. Besides, the possible reaction mechanism is proposed to explain the catalytic activity of the LSCO mesoporous nanowire.},
  doi       = {10.1073/pnas.1210315109},
  owner     = {amnesia},
  pmid      = {23150570},
  publisher = {Proceedings of the National Academy of Sciences},
  timestamp = {2015.03.25},
  url       = {http://www.pnas.org/content/109/48/19569.short},
}

@Article{ShimonishiSammes2011,
  author    = {Yuta Shimonishi and Tao Zhang and Nobuyuki Imanishi and Dongmin Im and Dong Joon Lee and Atsushi Hirano and Yasuo Takeda and Osamu Yamamoto and Nigel Sammes},
  title     = {A study on lithium/air secondary batteries{\textemdash}Stability of the {NASICON}-type lithium ion conducting solid electrolyte in alkaline aqueous solutions},
  journal   = {Journal of Power Sources},
  year      = {2011},
  volume    = {196},
  number    = {11},
  pages     = {5128--5132},
  month     = {jun},
  issn      = {03787753},
  abstract  = {The stability of the high lithium ion conducting glass ceramics, Li1+x+yTi2$-$xAlxSiyP3$-$yO12 (LTAP) in alkaline aqueous solutions with and without LiCl has been examined. A significant conductivity decrease of the LTAP plate immersed in 0.057M LiOH aqueous solution at 50$\deg$C for 3 weeks was observed. However, no conductivity change of the LTAP plate immersed in LiCl saturated LiOH aqueous solutions at 50$\deg$C for 3 weeks was observed. The pH value of the LiCl--LiOH--H2O solution with saturated LiCl was in a range of 7--9. The molarity of LiOH and LiCl in the LiOH and LiCl saturated aqueous solution were estimated to be 5.12 and 11.57M, respectively, by analysis of Li+ and OH$-$. The high concentration of LiOH and the low pH value of 8.14 in this solution suggested that the dissociation of LiOH into Li+ and OH$-$ is too low in the solution with a high concentration of Li+. These results suggest that the water stable LTAP could be used as a protect layer of the lithium metal anode in the lithium/air cell with LiCl saturated aqueous solution as the electrolyte, because the content of OH$-$ ions in the LiCl saturated aqueous solution does not increase via the cell reaction of Li+1/2O2+H2O$\rightarrow$2LiOH, and LTAP is stable under a deep discharge state.},
  doi       = {10.1016/j.jpowsour.2011.02.023},
  keywords  = {Alkaline solution,Anode,Lithium/air battery,Solid electrolyte},
  publisher = {Elsevier {BV}},
  url       = {http://www.sciencedirect.com/science/article/pii/S0378775311003776},
}

