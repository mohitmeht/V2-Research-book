@Article{BalasubramanianJoerissen2015,
  author    = {P. Balasubramanian and M. Marinaro and S. Theil and M. Wohlfahrt-Mehrens and L. Jörissen},
  title     = {Au-coated carbon electrodes for aprotic Li{\textendash}O2 batteries with extended cycle life: The key issue of the Li-ion source},
