@Article{OwejanJacobson2012,
  author        = {Jon P. Owejan and Jeffrey J. Gagliardo and Stephen J. Harris and Howard Wang and Daniel S. Hussey and David L. Jacobson},
  title         = {Direct measurement of lithium transport in graphite electrodes using neutrons},
  journal       = {Electrochimica Acta},
  year          = {2012},
  volume        = {66},
  pages         = {94--99},
  month         = {apr},
  issn          = {00134686},
  abstract      = {Lithium intercalation into graphite electrodes is widely studied, but few direct in situ diagnostic methods exist. Such diagnostic methods are desired to probe the influence of factors such as charge rate, electrode structure and solid electrolyte interphase layer transport resistance as they relate to lithium-ion battery performance and durability. In this work, we present a continuous measurement of through-plane lithium distributions in a composite graphite/lithium metal electrochemical cell. Capacity change in a thick graphite electrode was measured during several charge/discharge cycles with high resolution (14$\mu$m) neutron imaging. A custom test fixture and a method for quantifying lithium are described. The measured lithium distribution within the graphite electrode is given as a function of state of charge. Bulk transport resistance is considered by comparing intercalation rates through the thickness of the electrode near the separator and current collector. The residual lithium content associated with irreversible capacity loss that results from cycling is also measured.},
  doi           = {10.1016/j.electacta.2012.01.047},
  keywords      = {Battery,Graphite,Intercalation,Li-ion,Lithium,Neutron,Neutron Scattering,Transport},
  mendeley-tags = {Li-ion,Neutron Scattering},
  owner         = {amnesia},
  publisher     = {Elsevier {BV}},
  timestamp     = {2015.03.25},
  url           = {http://www.sciencedirect.com/science/article/pii/S001346861200076X},
}

