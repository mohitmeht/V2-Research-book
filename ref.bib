  isbn      = {1531-7331},
  keywords  = {cathode,current distribution,experimental and numerical investigations,have con-,mechanisms taking,microelectrode,mixed conducting,reaction mechanism,s abstract several recent,simulation,tributed to the improved,understanding of the electrochemical},
